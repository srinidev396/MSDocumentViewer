<?xml version="1.0" encoding="utf-8"?>
<configuration>

   <system.webServer>
      <handlers>
         <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModule" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath=".\DocumentService.exe" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" />
      <security>
         <requestFiltering>
            <requestLimits maxAllowedContentLength="524288000" />
         </requestFiltering>
      </security>
   </system.webServer>


</configuration>